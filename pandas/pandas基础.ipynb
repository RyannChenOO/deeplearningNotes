  {
   "cell_type": "code",
   "execution_count": 45,
   "id": "8ddc1624",
   "metadata": {},
   "outputs": [],
   "source": [
    "# data shape(3,4)--(列,行)\n",
    "data = [[1, 'a', 1.2], [2, 'b', 2.2], [3, 'c', 3.2],[4, 'd', 4.2]]\n",
    "df = pd.DataFrame(data=data, \n",
    "                  index=[\"row_%d\"%i for i in range(4)], \n",
    "                  columns=['col_0', 'col_1', 'col_2'])"
